<table class="table">
	<thead>
		<tr>
			<th>
			</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th>Admin</th>
		</tr>
	</thead>
  <% @user.each_with_index do |user, i| %>
    <tbody>
  	  <td><%= (i+1) %></td>
    	<td><%= user.first_name %></td>
    	<td><%= user.last_name %></td>
    	<td><%= user.email %></td>
    	<td><%= user.admin? %></td>
  <% end %>
  </tbody>
</table>

<%= link_to "Back", admin_dashboard_path %>