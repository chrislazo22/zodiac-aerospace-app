<div class="container">
  <h1>Search Results</h1>
  <% if @search.blank? %>
    <h4>There are no materials containing <%= params[:search] %>.</h4>
    <% else %>
    <table class="table">
    	<thead>
    		<tr>
    			<th>
    			</th>
    			<th>Test Plan</th>
    			<th>Cushion</th>
    			<th>Seat Model</th>
    			<th>Foam A</th>
    			<th>Foam B</th>
    			<th>Fireblock Type</th>
    		</tr>
    	</thead>
    	<tbody>
        <%= render @search.specimen %>
      </tbody>
    </table>
  <% end %>
</div>