<%= form_tag(specimen_path, :method => 'get') do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search" %>
  <%= submit_tag "Search", :name => nil %>
<% end %>

<% if current_user.admin? %>
  <%= link_to "Back", data_entry_path %>
<% else %>
  <%= link_to "Back", root_path %>
<% end %>
  